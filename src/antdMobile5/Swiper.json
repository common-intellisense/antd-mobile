      "default": "true",
      "value": "",
      "type": "boolean"
    },
    "autoplay": {
      "description": "Whether to switch automatically",
      "description_zh": "是否自动切换",
      "default": "false",
      "value": "",
      "type": "boolean"
    },
    "autoplayInterval": {
      "description": "Automatic switching interval, the unit is ms",
