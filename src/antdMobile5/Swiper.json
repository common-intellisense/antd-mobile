      "value": "",
      "type": "number"
    },
    "stopPropagation": {
      "description": "Stop the propagation of some events.",
      "description_zh": "停止某些事件的传播。",
      "default": "[]",
      "value": "",
      "type": "PropagationEvent[]"
    }
